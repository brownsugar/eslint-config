{
  "name": "@brownsugar/eslint-config",
  "version": "3.0.0-rc.3",
  "description": "Preferred ESLint configs for myself.",
  "author": "Lay",
  "license": "MIT",
  "homepage": "https://github.com/brownsugar/eslint-config",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "postversion": "git push && git push --tags"
  },
  "exports": {
    ".": {
      "import": "./dist/typescript.mjs",
      "require": "./dist/typescript.cjs"
    },
    "./typescript": {
      "import": "./dist/typescript.mjs",
      "require": "./dist/typescript.cjs"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs"
    }
  },
  "main": "./dist/typescript.cjs",
  "module": "./dist/typescript.mjs",
  "types": "./dist/typescript.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brownsugar/eslint-config.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.3.0",
    "rollup": "^4.18.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.3.13",
    "@stylistic/eslint-plugin": "^2.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.3.0",
    "typescript-eslint": "^7.11.0"
  },
  "peerDependencies": {
    "eslint": "^9",
    "nuxt": "^3",
    "vue": "^3"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.1.3"
}
