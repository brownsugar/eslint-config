{
  "name": "@brownsugar/eslint-config",
  "version": "3.2.1",
  "description": "Preferred ESLint configs for myself.",
  "author": "Lay",
  "license": "MIT",
  "homepage": "https://github.com/brownsugar/eslint-config",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --fix",
    "postversion": "git push && git push --tags"
  },
  "exports": {
    ".": {
      "import": "./dist/typescript.mjs",
      "require": "./dist/typescript.cjs",
      "types": "./dist/typescript.d.ts"
    },
    "./typescript": {
      "import": "./dist/typescript.mjs",
      "require": "./dist/typescript.cjs",
      "types": "./dist/typescript.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs",
      "types": "./dist/vue.d.ts"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "types": "./dist/nuxt.d.ts"
    }
  },
  "main": "./dist/typescript.cjs",
  "module": "./dist/typescript.mjs",
  "types": "./dist/typescript.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brownsugar/eslint-config.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.14.0",
    "rollup": "^4.27.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.7.0",
    "@stylistic/eslint-plugin": "^2.10.1",
    "eslint-plugin-vue": "^9.31.0",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.14.0"
  },
  "peerDependencies": {
    "eslint": "^9",
    "nuxt": "^3",
    "vue": "^3"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.13.2"
}
